set sandbox none
set statusbar-h-padding 0
set statusbar-v-padding 0
set page-padding 1
map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map i recolor
map p print

set default-bg                     "#0d0f19"
set default-fg                     "#b8b7b7"
set statusbar-bg                   "#0d0f19"
set statusbar-fg                   "#b8b7b7"
set inputbar-bg                    "#0d0f19"
set inputbar-fg                    "#b8b7b7"
set notification-bg                "#0d0f19"
set notification-fg                "#b8b7b7"
set notification-error-bg          "#0d0f19"
set notification-error-fg          "#b8b7b7"
set notification-warning-bg        "#0d0f19"
set notification-warning-fg        "#b8b7b7"
set highlight-color                "#796EA6"
set highlight-active-color         "#7874C3"
set completion-bg                  "#796EA6"
set completion-fg                  "#7874C3"
set completion-highlight-bg        "#796EA6"
set completion-highlight-fg        "#7874C3"
set recolor-lightcolor             "#0d0f19"
set recolor-darkcolor              "#b8b7b7"
